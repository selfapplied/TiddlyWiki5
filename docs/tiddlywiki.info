{
    "description": "Aemergent Wiki",
    "title": "Aemergent Wiki",
    "plugins": [
        "tiddlywiki/markdown",
        "tiddlywiki/katex",
        "tiddlywiki/highlight",
        "tiddlywiki/codemirror",
        "tiddlywiki/codemirror-mode-markdown",
        "tiddlywiki/freelinks"
    ],
    "themes": [
        "tiddlywiki/vanilla"
    ],
    "build": {
        "index": [
            "--rendertiddler",
            "$:/core/save/all",
            "index.html",
            "text/plain"
        ],
        "test": [
            "--test",
            "--rendertiddler",
            "$:/core/save/all",
            "test.html",
            "text/plain"
        ]
    },
    "palettes": [
        "$:/palettes/Vanilla",
        "$:/palettes/FlexokiDark"
    ]
} 
